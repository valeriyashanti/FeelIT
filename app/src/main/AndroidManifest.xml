<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.login.valeriya.feelit">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".AboutActivity" />
        <activity android:name=".Fragments.SubscriptionFragment" />
        <activity
            android:name=".ProfileFillActivity"
            android:label="@string/title_activity_profile_fill"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".SubscriptionActivity"></activity>
    </application>

</manifest>